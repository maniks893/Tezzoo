<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.VasMobitec">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/download"
        android:label="VasMobitec"
        android:roundIcon="@drawable/bg_2"
        android:supportsRtl="true"
        android:theme="@style/Theme.Teezoo">
        <activity android:name="com.example.VasMobitec.dashboard">

        </activity>
        <activity android:name="com.example.VasMobitec.OtpActivity" />
        <activity android:name="com.example.VasMobitec.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.VasMobitec.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>